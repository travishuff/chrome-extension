{
	"manifest_version": 2,
	"name": "Extension Name",
	"description": "Extension Description",
	"version": "1.0",
	"permissions": ["storage"],
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"content_scripts": [
		{
			"matches": ["http://www.yelp.com/biz/*"],
			"js": ["content.js"]
		}
	],
	"browser_action": {
			"default_icon": "icon.png",
			"default_title": "Chrome Extension Title",
			"default_popup": "popup.html"
		}
	}